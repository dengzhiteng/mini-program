/**app.wxss**/
@import './styles/iconfont.wxss';

page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;    
  margin: 0;
  box-sizing: border-box;
}
page{
  /* 定义主题颜色 */
  --themeColor: #eb4450;
  --textColor:#333;
  --titleColor:#000;
  --iconfontColor:#999;
  --textFontSize:28rpx;
  --borderColor:#dcdcdc;
  /* 定义统一字体大小 */
  font-size: 28rpx;
}
